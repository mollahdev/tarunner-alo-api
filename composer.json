{
    "name": "ashraf/tarunner-alo-api",
    "scripts": {
        "build": [
            "composer --no-plugins --no-interaction install --no-scripts --no-dev"
        ]
    },
    "autoload": {
        "psr-4": {
            "Tarunner_Alo\\App\\": "app/",
            "Tarunner_Alo\\Model\\": "model/",
            "Tarunner_Alo\\Controller\\": "controller/",
            "Tarunner_Alo\\Providers\\Sms\\": "providers/sms/",
            "Tarunner_Alo\\Providers\\Email\\": "providers/email/"
        }
    },
    "require": {
        "rakit/validation": "^1.4",
        "firebase/php-jwt": "^6.9",
        "phpmailer/phpmailer": "^6.8"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
